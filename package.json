    "ethereumjs-tx": "^2.0.0",
    "fast-water": "1.0.2",
    "fastify": "1.14.1",
    "fastify-cookie": "2.1.2",
    "fastify-cors": "1.0.1",
    "fastify-env": "0.6.2",
    "fastify-file-upload": "1.0.0",
    "fastify-jwt": "0.10.0",
    "fastify-static": "1.1.0",
    "fastify-swagger": "1.2.0",
    "lodash": "4.17.11",
