    "prod": "export NODE_ENV=production && node index",
    "testing": "npm run migrate && npm run prod",
    "get-flyway": "./get_flyway.sh",
    "migrate": "flyway -configFiles=flyway.conf -locations=filesystem:db/migrations migrate",
    "clean": "flyway -configFiles=flyway.conf -locations=filesystem:db/migrations clean"
  },
