{
  "name": "circle-of-angels-backend",
  "version": "1.0.0",
  "description": "Api-rest for circle of angels",
  "main": "main.js",
  "scripts": {
    "lint": "eslint src/**/*.js",
    "test": "export NODE_ENV=test && jest src/*",
    "test:coverage": "npm test -- --coverage",
